<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="hellofx.controllers.StudentMarksController"
      spacing="18" alignment="CENTER" style="-fx-background-color: #181A20;" prefWidth="1024" prefHeight="640">
    <Label text="Marks" style="-fx-font-size: 24px; -fx-text-fill: #2d75e8;"/>
    <TableView fx:id="marksTable" prefWidth="800">
        <columns>
            <TableColumn fx:id="courseColumn" text="Course"/>
            <TableColumn fx:id="semColumn" text="Semester"/>
            <TableColumn fx:id="marksColumn" text="Marks"/>
        </columns>
    </TableView>
</VBox>